{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "ignorePaths": ["**/ui/**"],
  "regexManagers": [
    {
      "fileMatch": ["build-images.sh"],
      "matchStrings": [
        "docker\\.io/collabora/code:(?<currentValue>[^\\s]+)"
      ],
      "datasourceTemplate": "docker",
      "depNameTemplate": "collabora/code"
    }
  ]
}
